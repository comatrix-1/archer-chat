{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "React Library",
	"include": ["."],
	"exclude": ["dist", "build", "node_modules"],
	"compilerOptions": {
		"strict": true,
		"alwaysStrict": true,
		"jsx": "preserve",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"noEmit": true,
		"declaration": false,
		"declarationMap": false
	}
}
